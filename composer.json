{
    "name": "phpmentors/pageflower-bundle",
    "type": "symfony-bundle",
    "description": "A pageflow engine for Symfony applications",
    "keywords": ["symfony", "flow", "pageflow", "stateful", "conversation"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "doctrine/annotations": "~1.0",
        "phpmentors/domain-kata": "~1.0",
        "piece/stagehand-fsm": "~2.4@dev",
        "symfony/config": "~2.7",
        "symfony/console": "~2.7",
        "symfony/dependency-injection": "~2.6,>=2.6.5",
        "symfony/form": "~2.7",
        "symfony/framework-bundle": "~2.7",
        "symfony/http-foundation": "~2.7",
        "symfony/http-kernel": "~2.7",
        "symfony/routing": "~2.7",
        "symfony/security": "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "piece/stagehand-testrunner": ">=4.0.1",
        "symfony/browser-kit": "~2.7",
        "symfony/css-selector": "~2.7",
        "symfony/filesystem": "~2.7",
        "symfony/twig-bridge": "~2.7",
        "symfony/twig-bundle": "~2.7",
        "symfony/validator": "~2.7"
    },
    "suggest": {
        "symfony/twig-bridge": ">=2.7.0 provides the `ConversationContext` object as `app.conversation` in Twig templates",
        "symfony/twig-bundle": ">=2.7.0 provides the `ConversationContext` object as `app.conversation` in Twig templates"
    },
    "autoload": {
        "psr-4": {
            "PHPMentors\\PageflowerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPMentors\\PageflowerBundle\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
